{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "distask.dispatcher": {
    "BrokerClientConfiguration": {
      "RerouteRetryCount": 5,
      "LastRoutedTimeThreshold": "00:00:15",
      "Resiliency": {
        "Enabled": false
      }
    }
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=localhost\\sqlexpress; Database=TaskDispatcherLog; Integrated Security=SSPI;",
          "tableName": "Log"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },

  "IntegrationTestHost": {
    "numberOfTasks": 5,
    "availabilityChecker": {
      "name": "HealthLevelChecker",
      "type": "Distask.TaskDispatchers.AvailabilityCheckers.HealthLevelChecker, Distask, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
      "parameters": [
        {
          "name": "logger",
          "value": "$ref"
        },
        {
          "name": "minLevel",
          "value": "7"
        }
      ]
    },
    "router": {
      "name": "RandomizedRouter",
      "type": "Distask.TaskDispatchers.Routing.RandomizedRouter, Distask, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
      "parameters": [
        {
          "name": "logger",
          "value": "$ref"
        }
      ]
    }
  }
}